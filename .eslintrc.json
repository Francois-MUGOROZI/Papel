{
  "env": {
    "browser": true,
    "es6": true,
    "mocha": true
  },
  "plugins": ["prettier"],
  "extends": ["airbnb-base", "prettier"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "import/no-cycle": 0,
    "prettier/prettier": "error",
    "no-unused-vars": "warn"
  }
}
